 = params || {}\n    switch (popupName) {\n    case POPUP.NAME.USER.LOGOUT:\n        return <PopupLogout {...params} />\n    case POPUP.NAME.USER.DELETE_USER:\n        return <PopupDeleteUser {...params} />\n    case POPUP.NAME.USER.EDIT_USER:\n        return <PopupEditUser {...params} />\n    case POPUP.NAME.IN_STREAM_ADS.CREATE_INSTREAM_ADS:\n        return <PopupCreateNewAds {...params} />\n    case POPUP.NAME.IN_STREAM_ADS.CREATE_NEW_INSTREAM_ADS:\n        return <PopupCreateNewInStreamAds {...params} />\n    case POPUP.NAME.IN_STREAM_ADS.DELETE_INSTREAM_ADS:\n        return <PopupDeleteAds {...params} />\n    case POPUP.NAME.IN_STREAM_ADS.DUPLICATE_INSTREAM_ADS:\n        return <PopupDuplicateAds {...params} />\n    case POPUP.NAME.IN_STREAM_ADS.EDIT_INSTREAM_ADS:\n        return <PopupEditAds {...params} />\n    case POPUP.NAME.CONTENT.EDIT_CONTENT:\n        return <PopupEditContentList {...params} />\n    case POPUP.NAME.CONTENT.ADD_CONTENT:\n        return <PopupAddContentList {...params} />\n    default:\n        return null\n}\n})\nexport default Popup\n","import React, {useEffect} from 'react'\nimport {\n  TheContent,\n  TheSidebar,\n  TheHeader\n} from './index'\n\nimport { useDispatch, useSelector} from 'react-redux'\nimport {getUser} from \"../actions/user\";\nimport {setToken} from \"../actions/app\";\nimport { ToastContainer, toast } from 'react-toastify'\nimport 'react-toastify/dist/ReactToastify.css'\nimport Popup from \"../views/instream-ads/Popup/Popup\";\nimport {useLocation} from \"react-router\";\nimport {useHistory} from \"react-router-dom\";\nimport Cookie from \"react-cookies\";\nimport ConfigCookie from \"../config/ConfigCookie\";\n\n\nconst TheLayout = () => {\n\n    const dispatch = useDispatch()\n    const dataProfile = useSelector(state => state?.User)\n    const location = useLocation();\n    const history = useHistory()\n    const currentPath = location.pathname\n    const search = location.search\n\n\n    let checkUserLogin = Cookie.load(ConfigCookie.getTokenKey())\n    useEffect(()=>{\n        const checkLogin = () => {\n            if (checkUserLogin){\n                dispatch(setToken())\n                dispatch(getUser())\n                history.push(\n                    currentPath !== '/'\n                        ? (currentPath + search)\n                        : '/instream-ads/content-provider-list'\n                )\n            }else {\n                history.push('/login?rel=' + (currentPath + search))\n            }\n        }\n        checkLogin()\n        // eslint-disable-next-line react-hooks/exhaustive-deps\n    },[checkUserLogin])\n\n    return(\n      <div className=\"c-app c-default-layout\">\n        <TheHeader profile={dataProfile?.PROFILE}/>\n        <TheSidebar/>\n        <div className=\"c-wrapper\" style={{marginTop: '3rem'}}>\n          <div className=\"c-body\">\n            <TheContent/>\n          </div>\n        </div>\n        <Popup/>\n        <ToastContainer\n          position={toast.POSITION.TOP_RIGHT}\n          autoClose={3000}\n          hideProgressBar={true}\n          newestOnTop={false}\n          closeOnClick\n          rtl={false}\n          pauseOnFocusLoss\n          draggable\n          pauseOnHover\n\n        />\n      </div>\n    )\n}\n\nexport default TheLayout\n\n"]}}